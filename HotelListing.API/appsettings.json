  "Serilog": {
    "Minimumlevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-txt",
          "rollingIntervel": "Day"
        }
      }
      //{
      //  "Name": "Seq",
      //  "Args": { "serverUrl": "http://localhost:5028" }
      //}
    ]

  },
  "AllowedHosts": "*"
}
